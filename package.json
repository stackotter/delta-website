		"eslint-plugin-svelte": "^2.34.0",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"tslib": "^2.0.0",
		"typescript": "^5.2.2",
		"vite": "^4.5.0"
	},
	"type": "module",
	"dependencies": {
		"svelte-scrollto": "^0.2.0",
